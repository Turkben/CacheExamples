@model HashSet<string>
@{
    ViewData["Title"] = "Detail";
}

<h1>Set </h1>


<form asp-action="Add" enctype="application/x-www-form-urlencoded" method="post">

    <input type="text" name="name" />
    <button type="submit">Add</button>
</form>

@foreach (var item in Model)
{
    
    <h3>@item<a asp-controller="Set" asp-action="Remove" asp-route-name="@item">Remove</a> </h3> 
}    